<Window x:Class="RRViewer1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Данные" >
                <MenuItem Header="Открыть" Click="MenuItemOpenFile_Click" />
                <MenuItem Header="Сохранить csv" Click="MenuItemSaveCSV_Click"/>
                <MenuItem Header="Сохранить tab" Click="MenuItemSaveTAB_Click" />
                <MenuItem Header="Сохранить mif" Click="MenuItemSaveMIF_Click" />
                <MenuItem x:Name="MI_Kml" Header="Сохранить kml" Visibility="Hidden" Click="MI_Kml_Click"/>
                <MenuItem Header="Закрыть" />
            </MenuItem>
            <MenuItem Header="Схема"/>
            <MenuItem x:Name="MI_Projection" Header="СК" Click="MI_Projection_Click"/>
        </Menu>
        <TabControl Name="TabControl1" />
    </DockPanel>
</Window>
